{

    "manifest_version": 2,
    "name": "Anki and Python Tutor - Dark Mode",
    "version": "1.0",
  
    "description": "Switches to dark mode for all webpages matching Anki Web or Python Tutor.",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.ankiweb.net/*", "*://*.ankiuser.net/*", "*://*.pythontutor.com/*"],
        "js": ["dark-mode.js"]
      }
    ]
  
}
  